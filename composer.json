{
    "name": "xavierau/multi-auth",
    "description": "The additional admin Auth Guard",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "auth",
        "laravel"
    ],
    "authors": [
        {
            "name": "Xavier Au",
            "email": "xavier.au@anacreation.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Anacreation\\MultiAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anacreation\\MultiAuth\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "5.5.*|5.6.*",
        "fico7489/laravel-pivot": "3.*",
        "laravel/passport": "~4.0|~5",
        "paragonie/random_compat": "2.*",
        "laravelcollective/html": "5.5.*|5.6.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anacreation\\MultiAuth\\MultiAuthServiceProvider"
            ]
        }
    }
}
